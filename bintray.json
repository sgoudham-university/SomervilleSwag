{
  "package": {
    "name": "${project.name}",
    "repo": "maven",
    "subject": "sgoudham",
    "website_url": "${project.url}",
    "issue_tracker_url": "${project.url}/issues",
    "vcs_url": "${project.url}",
    "licenses": ["Apache-2.0"],
    "labels": ["sgoudham", "somervilleswag"],
    "public_download_numbers": true,
    "public_stats": true
  },
  "version": {
    "name": "${project.version}",
    "released": "${maven.build.timestamp}",
    "vcs_tag": "${project.version}"
  },
  "files":
  [
    {"includePattern": "target/(.*)", "excludePattern": "target/.*.zip$", "uploadPattern": "/$1"}
  ],
  "publish": true
}